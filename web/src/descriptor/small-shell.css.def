body {
  font-family: sans-serif;
  color: #000333;
  letter-spacing: .04em;
  font-size: 94%;
  height: 100%;
  margin: 0;
  background: #fffafa;
}

@media screen and (min-width: 641px) {
  .container {
    margin: 0 auto;
    padding-top: 8px;
    padding-left: 48px;
    padding-right: 48px;
}
}

@media screen and (max-width: 640px) {
  .container {
    margin: 0 auto;
    padding-left: 12px;
    padding-right: 12px;
}
}

a {
  text-decoration: none;
  color: #000333;
}

.container * {
  box-sizing: border-box;
}

@media screen and (min-width: 641px) {
  .container .flex-header {
    margin-right: calc(50% - 50vw);
    margin-left: calc(50% - 50vw);
    padding-left: 48px;
    padding-right: 48px;
    display:flex;
    flex-wrap:wrap;
    margin-top: -10px;
    margin-bottom: -4px;
    background: #434246;
    color: #dcdcdc;
    height: 60px;
  }

  .container .left-header {
    font-weight: 300;
    line-height: 0.8em;
    font-size: 80%;
    display:flex;
    flex: auto;
    align-items: baseline;
    margin: auto;
  }

  .container .left-header .dataselect{
    font-weight: bold;
    background: #434246;
    color: #dcdcdc;
    cursor: hand;
    border-style: none;
  }

  .container .left-header p {
    font-weight: bold;
    padding: .3rem;
  }

  .container .left-header a {
    color: #6495ED;
  }


  .container .right-header {
    font-weight: 300;
    line-height: 0.8em;
    padding-right: 0px;
    margin: auto;
  }

  .container .right-header a {
    color: #ADFBEA;
    text-decoration: none;
    white-space: nowrap;
  }

  .container .right-header ul {
    list-style: none;
    margin: 0px;
    padding: 0px;
    margin-bottom: 28px;
  }

  .container .right-header li {
    margin: 2rem 0;
  }

  .container .right-header nav {
    background: #000011;
    opacity: 0.8;
    position: absolute;
    z-index: 1;
    top: 3.125rem;
    right: 0;
    overflow-x: hidden;
    text-align: center;
    display: none;
  }
}

@media screen and (max-width: 640px) {
  .container .flex-header {
    display:flex;
    flex-wrap:wrap;
    margin-bottom: -16px;
  }

  .container .left-header {
    font-weight: 300;
    line-height: 0.8em;
    font-size: 80%;
    display:flex;
    flex: auto;
    align-items: baseline;
    margin: auto;
  }

  .container .left-header .dataselect{
    font-weight: bold;
    color: #333333;
    cursor: hand;
    border-style: none;
  }

  .container .left-header p {
    font-weight: bold;
    padding: .3rem;
  }

  .container .left-header a {
    color: #6495ED;
  }


  .container .right-header {
    font-weight: 300;
    line-height: 0.8em;
    padding-right: 0px;
    margin: auto;
    color: white;
  }

  .container .right-header a {
    color: #ADFBEA;
    text-decoration: none;
    white-space: nowrap;
  }

  .container .right-header ul {
    list-style: none;
    margin: 0px;
    padding: 0px;
    margin-bottom: 28px;
  }

  .container .right-header li {
    margin: 2rem 0;
  }

  .container .right-header nav {
    background: #000011;
    padding-top: 10px;
    opacity: 0.8;
    position: absolute;
    z-index: 1;
    top: 3.125rem;
    right: 0;
    overflow-x: hidden;
    text-align: center;
    display: none;
  }
}

.flex-header a:hover{
  color : #4682b4;
}

@media screen and (min-width: 641px) {
  .console-container {
    margin-right: calc(50% - 50vw);
