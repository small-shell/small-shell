.tooltip {
  position: relative;
  cursor: pointer;
  color: #5ba4e9;
  opacity: 0.9;
}

@media screen and (min-width: 641px) {
  .tooltip-text {
    opacity: 0; 
    visibility: hidden; 
    position: absolute; 
    left: 340%; 
    padding-left: 12px;
    padding-right: 12px;
    transform: translateX(-50%);
    display: inline-block;
    white-space: nowrap; 
    font-size: 1.0rem;
    line-height: 1.3;
    background: #333;
    color: #dcdcdc;
    border-radius: 8px;
    transition: 0.3s ease-in;
  }
}

@media screen and (max-width: 640px) {
  .tooltip-text {
    opacity: 0; 
    visibility: hidden; 
    position: absolute; 
    padding-left: 12px;
    padding-right: 12px;
    transform: translateX(-50%);
    display: inline-block;
    white-space: nowrap; 
    line-height: 1.3;
    background: #333;
    color: #dcdcdc;
    border-radius: 8px;
    transition: 0.3s ease-in;
  }
}

.tooltip:hover .tooltip-text {
  opacity: 1;
  visibility: visible;
}

