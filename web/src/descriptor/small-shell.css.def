}

.console-box li {
  display: flex;
  flex-wrap: wrap;
  margin-bottom: 12px;
  font-weight: bold;
}

.console-box li input[type="text"],
.console-box li input[type="search"] {
  flex: 1 1 auto;
  font-weight: 300;
  letter-spacing: .09em;
