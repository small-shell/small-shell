body {
  font-family: 'Hiragino Kaku Gothic Pro',sans-serif;
  color: #000333;
  letter-spacing: .04em;
  font-size: 94%;
  height: 100%;
}

@media screen and (min-width: 641px) {
  .container {
    margin: 0 auto;
    padding-left: 48px;
    padding-right: 48px;
}
}

@media screen and (max-width: 641px) {
  .container {
    margin: 0 auto;
}
}

a {
  text-decoration: none;
  color: #000333;
}

.container * {
  box-sizing: border-box;
}

.flex-header {
  display:flex;
  flex-wrap:wrap;
  margin-bottom: -16px;
}

.left-header {
  font-weight: 300;
  line-height: 0.8em;
  font-size: 80%;
  flex: auto;
  margin: auto;
}

.left-header .dataselect{
  font-weight: bold;
  color: #333333;
  cursor: hand;
  border-style: none;
}

.left-header p {
  font-weight: bold;
  margin-top: 8px;
  margin-left: 2px;
}

.left-header a {
  color: #333;
}

.right-header {
  font-weight: 300;
  line-height: 0.8em;
  padding-right: 0px;
  margin: auto;
  color: white;
}

.right-header a {
  color: white;
  text-decoration: none;
  white-space: nowrap;
}

.right-header ul {
  list-style: none;
  margin: 0px;
  padding: 0px;
  margin-bottom: 28px;
}

.right-header li {
  margin: 2rem 0;
}

.right-header nav {
  background: #000011;
  padding-top: 10px;
  opacity: 0.8;
  position: absolute;
  z-index: 2;
  top: 3.125rem;
  right: 0;
  overflow-x: hidden;
  text-align: center;
  display: none;
}

@media screen and (min-width: 641px) {
.right-header .even-btn-menu {
    margin: auto;
    border-style: none;
    background: transparent;
    outline:0;
    color:  #000033;
    font-size: 240%;
    cursor: pointer;
    letter-spacing: .01em;
    border-radius: 8px;
    padding: .5rem;
    cursor: pointer;
    line-height: 1;
}
}

@media screen and (max-width: 641px) {
  .right-header .even-btn-menu {
    border-color: #000033;
    border-style: none;
    background: transparent;
    outline:0;
    color:  #000033;
    font-size: 240%;
    cursor: pointer;
    letter-spacing: .01em;
    border-radius: 8px;
    padding: .5rem;
    cursor: pointer;
    line-height: 1;
}
}

@media screen and (min-width: 641px) {
  .right-header nav.open-menu {
    border-radius: 16px;
    font-size: 88%;
    font-weight: bold;
    margin-top: 30px;
    margin-right: 20px;
    width: 18%;
    display: block;
  }
}
@media screen and (max-width: 640px) {
  .right-header nav.open-menu {
    margin-top: 20px;
    width: 100%;
    display: block;
  }
}

@media screen and (min-width: 641px) {
  .auth-container {
    width: 100%;
    height: 80%;
    display: flex;
    justify-content: center;
    align-items: center;
  }
  .auth-form {
    flex: 1 1 auto;
    font-size: 80%;
    line-height: 0.8em;
    letter-spacing: .09em;
    max-width: 280px;
    text-align: left;
}
}

@media screen and (max-width: 641px) {
  .auth-container {
    width: 100%;
    height: 80%;
    display: flex;
    justify-content: center;
    align-items: center;
  }
  .auth-form {
    flex: 1 1 auto;
    font-size: 60%;
    line-height: 0.8em;
    letter-spacing: .09em;
    max-width: 240px;
    text-align: left;
}
}


.auth-form b {
  color: red;
}

.auth-form h1 {
  color: #2f3333;
  font-size: 240%;
}

.auth-form .wrapper {
  display: flex;
  padding-bottom: 8px;
}

.auth-form p {
  font-size: 80%;
}

.auth-form .wrapper input[type="text"], 
.auth-form .wrapper input[type="password"] {
  flex: 1 1 auto;
  font-weight: 300;
  letter-spacing: .09em;
  font-size: 80%;
  padding: 10px 16px;
  border-radius: 0px;
  border: 2px solid #c0c0c0;
}

.auth-form button {
  font-weight: bold;
  padding: 2px 16px;
  border-style: none;
  background: #0066CC;
  color: #f8f8f8;
  text-transform: uppercase;
  letter-spacing: .09em;
  border-radius: 2px;
  cursor: pointer;
  cursor: hand;
  font-size: 80%;
}

.console-box {
  font-weight: 300;
  line-height: 0.8em;
  letter-spacing: .09em;
  padding: 18px 2px;
  text-align: left;
  margin: auto;
}

.console-box b {
  color: #008b8b;
}

.console-box ul {
  list-style: none;
  margin: 0px;
  padding: 0px;
  margin-bottom: 32px;
}

.console-box li {
  display: flex;
  flex-wrap: wrap;
  margin-bottom: 12px;
  font-weight: bold;
}

.console-box li input[type="text"] {
  flex: 1 1 auto;
  font-weight: 300;
  letter-spacing: .09em;
  padding: 8px 16px;
  border: 2px solid #E6E6FA;
  border-radius: 3px;
  font-size: 80%;
}

.console-box button {
  padding: 8px 16px;
  border-style: none;
  background: #000033;
  color: #f2f2f2;
  text-transform: uppercase;
  letter-spacing: .09em;
  border-radius: 2px;
  cursor: pointer;
  cursor: hand;
}

.console-box pre {
  flex: auto;
  font-size: 12px;
  border-radius: 4px;
  line-height: 1.4em;
  margin: 0px;
  background: #2f3333;
  color: white;
  margin-top: -12px;
  min-height: 84px;
  padding: 8px 12px;
  overflow:auto;
}

.console-box pre b {
  color: orange;
  font-weight: normal;
}

.console-box a {
  color: #b0c4de;
}

.table-box {
  line-height: .04em;
  padding: 4px 2px;
  margin: auto;
  color: #111111;
  margin-bottom: -24px;
}

.table-box .description {
  padding-top: 12px;
  padding-bottom: 12px;
}

.table-box .description h1 {
  font-size: 280%;
}

.table-box .wrapper {
  display: flex;
}

.table-box .wrapper input[type="text"],
.table-box .wrapper input[type="search"] {
  flex: 1 1 auto;
  font-weight: 300;
  letter-spacing: .09em;
  padding: 8px 16px;
  border: 2px solid #E6E6FA;
  border-radius: 3px;
  font-size: 80%;
}

.table-box .tags {
  display: flex;
  color: #008b8b;
  font-weight: bold;
  font-size: 86%;
  flex-wrap: wrap;
  white-space: nowrap;
  margin : 0px ;
  padding : 0px ;
}

.table-box .tags p {
  padding-right: 8px;
  font-size: 80%;
}

.table-box .tags a {
  color: #6495ED;
}

.table-box .pagelink {
  display: flex;
  padding-top:4px;
  align-items: baseline;
  justify-content: flex-end;
  line-height: 1.2em;
  font-weight: bold;
  font-size: 72%;
}

.table {
  line-height: .8em;
  margin: auto;
}

.table-header{
  font-weight: 300;
  font-weight: bold;
  border-radius: 4px;
  background: #0066CC;
  color: white;
}

.table ul {
  list-style: none;
  margin: 0px;
  padding: 0px;
}

.table li {
  display: flex;
  flex-wrap: nowrap;
}

.table .odd_line {
  background:#FAFAFA;
}

.table .even_line {
  background:#FFFFFF;
}

.table li p {
  width: 120px;
  padding-left: 8px;
  font-size: 70%;
  flex: auto;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.table a {
  color: blue;
}

.table-box .pagelink a {
 color: blue;
}

@media screen and (min-width: 640px){ 
  .form-box {
    max-width: 94%;
    padding: 32px 2px;
    text-align: left;
    line-height: 0.8em;
    margin: auto;
}
}

@media screen and (max-width: 640px){ 
  .form-box {
    max-width: 100%;
    padding: 24px 2px;
    text-align: left;
    line-height: 0.8em;
    margin: auto;
}
}

.form-box a {
  color: #008;
}

.form-box .description {
  padding-top: 28px;
  text-align: left;
  line-height: 0.8em;
  margin-bottom: -10px;
}

.form-box .description h1 {
  font-size: 240%;
  color: #20b2aa;
}

.form-box .description h2 {
  color: #444444;
}

.form-box .description p {
  color: #696969;
}

.form-box .log {
  text-align: left;
  line-height: 1.2em;
  margin-top: 4px;
  margin-bottom: 12px;
  color: #696969;
}

.form-box hr {
  margin-top: -5px;
  margin-bottom: 12px;
  border: 0.5px dotted #d3d3d3;
}


.form-box ul {
  list-style: none;
  align-items: flex-start;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  margin: 0px;
  padding: 0px;
  margin-bottom: 12px;
}

@media screen and (max-width: 640px){ 
  .form-box li {
    width: 100%;
    display: flex;
    align-items: center;
    flex-wrap: wrap;
    min-width :220px;
    padding-top: 8px;
    padding-bottom: 8px;
}
}

@media screen and (min-width: 640px){ 
  .form-box li {
    width: 48%;
    display: flex;
    align-items: center;
    flex-wrap: wrap;
    min-width :220px;
    padding-top: 8px;
    padding-bottom: 8px;
}
}


@media screen and (max-width: 640px){
  .form-box li label:not(.checkbox-label):not(.radio-label):not(#file_input_label):not(#gray_label) {
    font-size: 70%;
    text-align: left;
    color: #444444;
    min-width:100%;
    font-weight: 300;
    margin-right: 2px;
    letter-spacing: .09em;
    padding: 6px 6px;
    font-weight: bold;
}
}

@media screen and (min-width: 640px){
  .form-box li label:not(.checkbox-label):not(.radio-label):not(#file_input_label):not(#gray_label) {
    font-size: 84%;
    font-weight:bold;
    color: #444444;
    min-width:100%;
    text-align: left;
    padding: 3px 3px;
    margin-right: 2px;
    letter-spacing: .09em;
}
}

.form-box li input:not([type='checkbox']):not([type='radio']):not([type='file']):not([type='date']):not([type='datetime-local']),
.form-box li textarea {
  letter-spacing: .09em;
  font-size: 12px;
  padding: 6px 6px;
  border: 2px solid #d3d3d3;
  flex: auto;
}

@media screen and (min-width: 640px){
  .form-box li select {
    letter-spacing: .09em;
    font-size: 12px;
    padding: 6px 6px;
    border: 2px solid #d3d3d3;
    flex: auto;
  }

  .form-box input[type="date"],
  .form-box input[type="datetime-local"] {
    letter-spacing: .09em;
    font-size: 12px;
    padding: 4px 6px;
    border: 2px solid #d3d3d3;
    flex: auto;
  }
}

@media screen and (max-width: 640px){
  .form-box li select {
    -webkit-appearance: none;
    -moz-appearance: none;
    appearance: none;
    letter-spacing: .09em;
    font-size: 12px;
    padding: 6px 6px;
    border: 2px solid #d3d3d3;
    flex: auto;
  }

  .form-box input[type="date"],
  .form-box input[type="datetime-local"] {
    letter-spacing: .09em;
    font-size: 12px;
    border: 4px solid #d3d3d3;
    flex: auto;
  }
}

.form-box li pre {
  margin-top: 0px;
  margin-bottom: 0px;
  border-radius: 12px;
  padding: 12px 12px;
  background: #e0ffff;
  min-width:100%;
}

.form-box input[type="radio"],
.form-box input[type="checkbox"]{
  margin-top: 6px;
}

.form-box li p {
  text-align: left;
  background: #e6e6fa;
  font-size: 12px;
  margin:0;
  padding: 6px 6px;
  border: 2px solid #d3d3d3;
  letter-spacing: .09em;
  flex: auto;
}

.form-box .file_form {
  padding-top: 12px;
}

.form-box #gray_label {
    font-size: 70%;
    text-align: left;
    color: #c0c0c0;
    min-width:100%;
    font-weight: 300;
    margin-right: 2px;
    letter-spacing: .09em;
    padding: 6px 6px;
    font-weight: bold;
}

.form-box .file_form #file_input_label {
  background-color: #0C47C1;
  cursor: pointer;
  color: white;
  font-weight: bold;
  padding-left: 6px;
  padding-right: 6px;
  border-radius: 20%;
}

.form-box .file_form #file_name {
  color: #333;
  margin-left: -2px;
}

.form-box .file_form a {
  color: #6495ED;
}

.form-box button {
  text-align: right;
  padding: 8px 16px;
  border-style: none;
  background: #20b2aa;
  color: #f2f2f2;
  text-transform: uppercase;
  letter-spacing: .09em;
  border-radius: 2px;
  cursor: pointer;
  cursor: hand;
}

.form-box .flex-button {
  display:flex;
  flex-wrap:wrap;
}

.form-box .flex-button button {
  flex: auto;
  font-weight: bold;
  text-align: center;
  padding: 4px 8px;
  border-style: none;
  background: #2f3333;
  color: #f2f2f2;
  text-transform: uppercase;
  letter-spacing: .09em;
  border-radius: 2px;
  cursor: pointer;
  cursor: hand;
}

.form-box .action_links {
  text-align: left;
  padding-top: 12px;
  line-height: 0.8em;
  font-size: 80%;
  color: #808080;
}

.form-box .action_links a {
  font-size: 80%;
  color: #808080;
}



.form-box .action_menu p {
  font-size: 80%;
  color: #c0c0c0;
}

.form-box .action_menu a {
  font-size: 80%;
  color: #c0c0c0;
}

.result-box {
  padding-top: 60px;
  padding-bottom: 40px;
}

.result-box pre {
  flex: auto;
  font-size: 140%;
  border-radius: 8px;
  line-height: 1.4em;
  margin: 0px;
  padding: 12px 12px;
  overflow:auto;
}

.result-box a {
  color: #649;
}

.log_viewer {
  padding-left: 12px;
  padding-top: 2px;
}

.log_viewer pre {
  flex: auto;
  font-size: 12px;
  line-height: 1.4em;
  margin: 0px;
  margin-top: -10px;
  overflow:auto;
}

.statistics_viewer {
  padding-left: 12px;
  padding-top: 2px;
}

.statistics_viewer pre {
  flex: auto;
  font-size: 12px;
  line-height: 1.4em;
  margin: 0px;
  margin-top: -10px;
  overflow:auto;
}

.import-form {
 text-align: center;
 padding-top: 84px;
 padding-bottom: 200px;
}

.import-form ul {
  list-style: none;
  justify-content: space-between;
  margin: 0px;
  padding: 4px 12px;
}


.import-form li select {
  letter-spacing: .09em;
  font-size: 12px;
  padding: 6px 6px;
  border-style: none;
}

.import-form li p {
  text-align: left;
  background: #e6e6fa;
  font-size: 12px;
  margin:0;
  padding: 12px 6px;
  border: 2px solid #d3d3d3;
  letter-spacing: .09em;
}

.import-form button {
  padding: 8px 16px;
  border-style: none;
  background: #2f3333;
  color: #f2f2f2;
  text-transform: uppercase;
  letter-spacing: .09em;
  border-radius: 2px;
  cursor: pointer;
  cursor: hand;
}

.import-form pre {
  font-size: 16px;
  line-height: 1.4em;
}

.import-form a {
 color: blue;
 font-weight: bold;
}

.import-form #file_input_label {
  color: white;  
  background-color: #333;
  padding-left: 6px;
  padding-right: 6px;
  border-radius: 50%;
}

.checkbox-label {
  text-align: left;
  font-size: 80%;
  margin-top: 6px;
  letter-spacing: .09em;
  padding-right: 2px;
}

.radio-label {
  text-align: left;
  margin-top: 6px;
  font-size: 80%;
}

input[type="file"] {
 display: none;
}

.main {
 padding-top: 24px;
 height: 68%;
 display: flex;
 flex-wrap: wrap;
 justify-content: center;
 align-items: flex-start;
}

.main .description {
  flex: none;
  max-width: 480px;
  text-align: left;
}

.main .description h1 {
  font-size: 400%;
}

.main .description ul {
  list-style: none;
  margin: 0px;
  padding: 0px;
  margin-bottom: 28px;
}

.main .calendar {
 flex: auto;
 z-index: 1;
 padding-top: 24px;
 padding-left: 24px;
 padding-right: 24px;
 line-height: 0.8em;
 letter-spacing: .09em;
 text-align: center;
}

.main a {
 color: #000080;
}

.main a {
 color: #000080; 
}

.footer {
  color: #696969;
  padding-top: 4px;
  padding-right: 12px;
  font-weight: 300;
  font-size: 60%;
  text-align: right;
  bottom: 0;
  width: 100%;
  font-weight: bold;
}
